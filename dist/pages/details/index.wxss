.flex-title {
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: nowrap;
  text-align: left;
  position: relative;
  display: flex;
  align-items: center;
}
.flex-title:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 300%;
  height: 300%;
  border: 0 #ccc solid;
  box-sizing: border-box;
  transform-origin: 0 0;
  transform: scale(0.33333333);
  pointer-events: none;
  z-index: 0;
  border-bottom-width: 1px;
}
.flex-title .label {
  margin: 20rpx 0;
