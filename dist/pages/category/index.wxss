.container {
  display: flex;
}
.imgbox {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin: 0 auto;
}
.imgbox .ui-image {
  width: 100%;
}
.tabs {
  padding: 30rpx 0;
  display: flex;
  justify-content: center;
  background: #F6F6F9;
}
.active {
  border: none;
  border-left: 3px solid red;
  background: #fff;
}
.small-title {
  font-size: 24rpx;
  text-align: center;
}
.titles {
  height: 60rpx;
  margin-top: 20rpx;
  margin-left: 15rpx;
  font-weight: bold;
}
.goods-box {
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  margin-bottom: 10rpx;
}
.goods-box .goods-list {
  margin: 0rpx 9rpx 20rpx 15rpx;
  background: #fff;
}
