.row {
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  position: relative;
  padding: 0 10px;
  display: flex;
  align-items: center;
}
.row .tip {
  position: absolute;
  right: 60rpx;
  color: #999999;
}
.row .head-image image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.row.head {
  height: 110rpx;
  line-height: 110rpx;
}
