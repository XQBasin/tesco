              <view class="total">¥89.00</view>
              <view class="number">x2</view>
            </view>
          </view>
        </view>
      </scroll-view>
    </swiper-item>
    <!-- 系统消息 -->
    <swiper-item>
      <scroll-view scroll-y style="height: 1100rpx;">
        <view class="list">
          <view class="icon">
            <image src="../../static/image/person/icon_message.png" />
          </view>
          <view class="info">
            <view>通知</view>
            <view class="detail">水电费很多事复合肥好阀口袋的发生第三煞风景...</view>
