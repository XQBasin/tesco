    <view class="img">
      <image src="../../static/image/person/icon_Courier.png" />
    </view>
    <view class="info">
      <view class="title">派送员</view>
      <view>卡卡西</view>
    </view>
    <view class="phone" bindtap="call">
      13335555552
      <span type="phone" color="#f87005" size="52rpx;" class="ui-icon icon-phone" style="color: #f87005;font-size: 52rpx;px;"></span>
    </view>
  </view>

  <!-- 物流过程 -->
  <view class="content">
    <view class="item success">
      <view class="green">
        <image src="../../static/image/person/icon_green.png" />
      </view>
