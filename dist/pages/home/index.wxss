.search {
  width: 90%;
  height: 28px;
  border-radius: 30px;
  padding: 2px 10px;
  position: absolute;
  /* 初始化为相对定位 滑动时改为绝对定位fixed*/
  top: 78px;
  left: 0;
  right: 0;
  /* 决定了搜索框置顶 */
  z-index: 2;
  background: rgba(255, 255, 255, 0.6);
  margin: auto;
  display: flex;
  align-items: center;
}
.search input {
  width: 100%;
